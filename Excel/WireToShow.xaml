<Window x:Class="Wiring.WireToShow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wiring"
        mc:Ignorable="d"
        Title="WireToShow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="43*"/>
            <ColumnDefinition Width="57*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="54*"/>
            <RowDefinition Height="53*"/>
            <RowDefinition Height="45*"/>
            <RowDefinition Height="48*"/>
            <RowDefinition Height="54*"/>
            <RowDefinition Height="47*"/>
            <RowDefinition Height="41*"/>
            <RowDefinition Height="40*"/>
            <RowDefinition Height="52*"/>
        </Grid.RowDefinitions>
        <Label x:Name="label1" Content="Label1"/>
        <Label x:Name="label2" Content="Label2" Margin="0,54,0,0" Grid.RowSpan="2"/>
        <Label x:Name="label3" Content="Label3" Grid.Row="2" Margin="0,4,0,0"/>
        <Label x:Name="label4" Content="Label4" Grid.Row="2" Margin="0,45,0,0" Grid.RowSpan="2"/>
        <Label x:Name="label5" Content="Label5" Grid.Row="3" Margin="0,48,0,0" Grid.RowSpan="2"/>
        <Label x:Name="label6" Content="Label6" Grid.Row="4" Margin="0,54,0,0" Grid.RowSpan="2"/>
        <Label x:Name="label7" Content="Label7" Grid.Row="5" Margin="0,47,0,0" Grid.RowSpan="2"/>
        <Label x:Name="label8" Content="Label6" Grid.Row="6" Margin="0,41,0,0" Grid.RowSpan="2"/>
        <Label x:Name="label9" Content="Label7" Grid.Row="7" Margin="0,40,0,0" Grid.RowSpan="2"/>
        <StackPanel Grid.Column="1" Grid.Row="3"/>
        <Button x:Name="button" Grid.Column="1" Content="Button" HorizontalAlignment="Left" Margin="344,20,0,10" Grid.Row="7" Grid.RowSpan="2" Width="102" Click="button_Click"/>

    </Grid>
</Window>
